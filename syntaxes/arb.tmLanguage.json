{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Application Resource Bundle",
	"scopeName": "source.arb",
	"patterns": [
		{
			"include": "#outerbrackets"
		}
	],
	"repository": {
		"listofpairs": {
			"name": "keyword",
			"patterns": [
				{
					"name": "entity.name.tag",
					"match": "\"id\""
				},
				{
					"match": ":"
				},
				{
					"name": "string",
					"include": ""
				}
			]
		},
		"letter": {
			"begin": "\"",
			"end": "\"",
			"match": "[^@][^\"]+",
			"name": "keyword.letter"
		},
		"outerbrackets": {
			"begin": "{",
			"end": "}",
			"beginCaptures": {
				"0": {
					"name": "punctuation.paren.open"
				}
			},
			"endCaptures": {
				"0": {
					"name": "punctuation.paren.close"
				}
			},
			"name": "expression.group",
			"patterns": [
				{
					"include": "#listofpairs"
				}
			]
		}
	}
}